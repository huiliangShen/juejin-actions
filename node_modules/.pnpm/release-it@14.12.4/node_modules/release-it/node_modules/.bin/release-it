#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/release-it@14.12.4/node_modules/release-it/bin/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/release-it@14.12.4/node_modules/release-it/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/release-it@14.12.4/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules:/Users/banruo/Documents/my/github/node_modules:/Users/banruo/Documents/my/node_modules:/Users/banruo/Documents/node_modules:/Users/banruo/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/release-it@14.12.4/node_modules/release-it/bin/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/release-it@14.12.4/node_modules/release-it/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/release-it@14.12.4/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules:/Users/banruo/Documents/my/github/node_modules:/Users/banruo/Documents/my/node_modules:/Users/banruo/Documents/node_modules:/Users/banruo/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/release-it.js" "$@"
else
  exec node  "$basedir/../../bin/release-it.js" "$@"
fi
