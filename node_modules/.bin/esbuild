#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/esbuild@0.14.14/node_modules/esbuild/bin/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/esbuild@0.14.14/node_modules/esbuild/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/esbuild@0.14.14/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules:/Users/banruo/Documents/my/github/node_modules:/Users/banruo/Documents/my/node_modules:/Users/banruo/Documents/node_modules:/Users/banruo/node_modules:/Users/node_modules:/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/esbuild/bin/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/esbuild/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/esbuild@0.14.14/node_modules/esbuild/bin/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/esbuild@0.14.14/node_modules/esbuild/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/esbuild@0.14.14/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/.pnpm/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules:/Users/banruo/Documents/my/github/node_modules:/Users/banruo/Documents/my/node_modules:/Users/banruo/Documents/node_modules:/Users/banruo/node_modules:/Users/node_modules:/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/esbuild/bin/node_modules:/Users/banruo/Documents/my/github/juejin-actions/node_modules/esbuild/node_modules"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
